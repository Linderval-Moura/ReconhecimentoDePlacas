{
    "version": 2,
    "builds": [
        {
            "src":"./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Content-Security-Policy",
              "value": "default-src 'self'; font-src 'self' https://fonts.gstatic.com;"
            }
          ]
        }
      ]
}