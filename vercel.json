{
    "version": 2,
    "builds": [
        {
            "src":"index.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "index.js": {
          "includeFiles": "node_modules/tesseract.js-core/tesseract-core-simd.wasm",
          "memory": 1024,
          "maxDuration": 10
        }
      },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}