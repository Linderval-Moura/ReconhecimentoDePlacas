{
    "version": 2,
    "builds": [
        {
            "src":"index.js",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "TESSDATA_PREFIX": "/var/task/node_modules/tesseract.js-core/"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}