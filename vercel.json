{
    "version": 2,
    "builds": [
        { "src": "Frontend/package.json", "use": "@vercel/static-build", "config": { "outputDirectory": "Frontend/build" } },
        { "src": "Backend/index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "Backend/$1" },
        { "src": "/(.*)", "dest": "Frontend/build/$1" }
    ]
}