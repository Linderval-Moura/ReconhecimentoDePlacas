{
    "version": 2,
    "builds": [
        {
            "src":"index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "index.js"
        }
    ],
    "files": [
        "index.js",
        "node_modules/tesseract.js-core/**",
        "node_modules/tesseract.js/**"
    ]
}