{
    "version": 2,
    "builds": [
        {
            "src":"./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Content-Security-Policy": "default-src 'self'; font-src 'self' https://fonts.gstatic.com;"
            },
            "dest": "/index.js"
        }
    ]
}