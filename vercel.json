{
    "version": 2,
    "builds": [
        {
            "src":"./Backend/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)", 
            "destination": "/"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./Backend/index.js"
        }
    ]
}